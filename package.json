{
    "name": "@gridsuite/commons-ui",
    "version": "0.20.5",
    "description": "common react components for gridsuite applications",
    "main": "lib/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "build": "npm install;nwb build-react-component --copy-files --no-demo;npm prune --production",
        "clean": "nwb clean-module && nwb clean-demo",
        "prepublishOnly": "npm run build",
        "start": "npm install;nwb serve-react-demo",
        "test": "nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server"
    },
    "dependencies": {
        "@svgr/webpack": "^5.4.0",
        "autosuggest-highlight": "^3.2.0",
        "clsx": "^1.0.4",
        "jwt-decode": "^3.0.0",
        "memoize-one": "^5.1.1",
        "oidc-client": "^1.10.1",
        "prop-types": "15.7.2",
        "react-csv-downloader": "^2.7.0",
        "react-request-fullscreen": "^1.1.2",
        "react-virtualized": "^9.21.2"
    },
    "peerDependencies": {
        "@emotion/react": "^11.8.1",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.5.0",
        "@mui/lab": "^5.0.0-alpha.72",
        "@mui/material": "^5.5.0",
        "@mui/styles": "^5.5.0",
        "notistack": "^2.0.3",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react": "^17.0.0",
        "react-dom": "^17.0.0",
        "react-intl": "^5.0.0"
