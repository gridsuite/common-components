        "type-fest": "^4.14.0",
        "typescript": "5.1.6",
        "utf-8-validate": "^6.0.3",
        "vite": "^5.2.7",
        "vite-plugin-dts": "^3.8.1",
        "vite-plugin-eslint": "^1.8.1",
