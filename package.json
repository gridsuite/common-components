{
    "name": "@gridsuite/commons-ui",
    "version": "0.6.1",
    "description": "common react components for gridsuite applications",
    "main": "lib/index.js",
    "files": [
        "css",
        "es",
        "lib",
        "umd"
    ],
    "scripts": {
        "build": "npm install;nwb build-react-component --copy-files --no-demo;npm prune --production",
        "clean": "nwb clean-module && nwb clean-demo",
        "prepublishOnly": "npm run build",
        "start": "npm install;nwb serve-react-demo",
        "test": "nwb test-react",
        "test:coverage": "nwb test-react --coverage",
        "test:watch": "nwb test-react --server"
    },
    "dependencies": {
        "oidc-client": "^1.10.1",
        "jwt-decode": "^2.2.0",
        "react-request-fullscreen": "^1.1.2",
        "prop-types": "15.7.2",
        "@svgr/webpack": "^5.4.0"
    },
    "peerDependencies": {
        "notistack": "^1.0.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/core": "^4.9.5",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-intl": "^3.11.0"
    },
    "devDependencies": {
        "notistack": "^1.0.1",
        "@material-ui/core": "^4.9.5",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.56",
        "babel-eslint": "^10.1.0",
        "bufferutil": "^4.0.1",
        "eslint": "^7.9.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.21.0",
        "eslint-plugin-react-hooks": "^4.1.2",
        "inferno": "^7.4.2",
        "nwb": "^0.25.x",
        "prettier": "2.0.5",
        "puppeteer": "^5.3.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-intl": "^3.11.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "type-fest": "^0.13.1",
        "utf-8-validate": "^5.0.2",
        "webpack-plugin-serve": "^1.0.1"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:prettier/recommended"
        ],
        "rules": {
            "prettier/prettier": "warn",
            "import/no-webpack-loader-syntax": "off"
        }
    },
    "author": "gridsuite team",
    "homepage": "https://github.com/gridsuite",
    "license": "MPL",
    "repository": "git://github.com/gridsuite/commons-ui.git"
}
